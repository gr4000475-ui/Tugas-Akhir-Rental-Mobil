public class Ferrari extends Mobil {

    public Ferrari() {
        super("Ferrari", 6000000);
    }

    @Override
    public int hitungSewa(int hari, boolean pakaiSupir) {
        int total = hargaPerHari * hari;
        if (pakaiSupir) {
            total += 500000 * hari;
        }
        return total;
    }
}
